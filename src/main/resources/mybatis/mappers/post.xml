<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="mapper.post">
		<resultMap id="postResult" type="postVO">
			<result property="sigungu" column="sigungu" />
			<result property="doro" column="doro" />
			<result property="dong" column="dong" />
		</resultMap>
	
	<select id="selectPostBysigungu" resultType="postVO" parameterType="String">
		<![CDATA[
			select * from postTest where sigungu = #{sigungu}
		]]>
	</select>
	
	<select id="selectPostBydoro" resultType="postVO" parameterType="String">
		<![CDATA[
			select * from postTest where doro = #{doro}
		]]>
	</select>
	
	<select id="selectPostBydong" resultType="postVO" parameterType="String">
		<![CDATA[
			select * from postTest where dong = #{dong}
		]]>
	</select>
	
	
</mapper>